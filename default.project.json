{
  "name": "softlocked",
  "tree": {
    "$className": "DataModel",
    "$ignoreUnknownInstances": true,

    "ReplicatedStorage": {
      "Remote": {
        "$className": "Folder",

        "OnTowerWin": {
          "$className": "RemoteEvent"
        },
        
        "RequestTower": {
          "$className": "RemoteFunction"
        },

        "StopTower": {
          "$className": "RemoteFunction"
        }
      },

      "Roact": {
        "$path": "src/shared/Roact"
      },

      "createInstance": {
        "$path": "src/shared/createInstance.lua"
      },

      "Signal": {
        "$path": "src/shared/Signal.lua"
      },

      "SoftlockedCommon": {
        "$path": "src/shared/SoftlockedCommon"
      }
    },

    "StarterGui": {
      "$path": "src/gui",
      "$ignoreUnknownInstances": true
    },

    "StarterPlayer": {
      "StarterCharacterScripts": {
        "$path": "src/character",
        "$ignoreUnknownInstances": true
      },

      "StarterPlayerScripts": {
        "SoftlockedClient": {
          "$path": "src/client"
        },

        "PlayerModule": {
          "$path": "src/PlayerModule"
        }
      }
    },

    "ServerScriptService": {
      "SoftlockedServer": {
        "$path": "src/server"
      }
    },

    "ServerStorage": {
      "Archive": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },

      "Levels": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      },

      "Winrooms": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      }
    },

    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}