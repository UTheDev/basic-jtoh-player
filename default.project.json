{
  "name": "softlocked",
  "tree": {
    "$className": "DataModel",
    "$ignoreUnknownInstances": true,

    "ReplicatedStorage": {
      "Remote": {
        "$className": "Folder",

        "OnTowerWin": {
          "$className": "RemoteEvent"
        },
        
        "RequestTower": {
          "$className": "RemoteFunction"
        },

        "StopTower": {
          "$className": "RemoteFunction"
        }
      },

      "createInstance": {
        "$path": "src/shared/createInstance.lua"
      },

      "Signal": {
        "$path": "lib/Signal.lua"
      },

      "Softlocked": {
        "$path": "src/shared"
      }
    },

    "StarterGui": {
      "$path": "src/gui",
      "$ignoreUnknownInstances": true
    },

    "StarterPlayer": {
      "StarterCharacterScripts": {
        "$path": "src/character",
        "$ignoreUnknownInstances": true
      },

      "StarterPlayerScripts": {
        "COScriptRepo": {
          "$className": "Folder",
          "$ignoreUnknownInstances": true,

          "JToH": {
            "$path": "lib/JToH/clientObjects"
          },

          "Softlocked": {
            "$path": "src/clientObjects"
          }
        },

        "Softlocked": {
          "$path": "src/client"
        },

        "PlayerModule": {
          "$path": "lib/PlayerModule"
        }
      }
    },

    "ServerScriptService": {
      "Softlocked": {
        "$path": "src/server/Softlocked"
      },

      "ServerMain": {
        "$path": "src/server/ServerMain.server.lua"
      },

      "JToH": {
        "$path": "lib/JToH/server"
      },

      "StudioNoclip": {
        "$path": "src/server/StudioNoclip"
      }
    },

    "ServerStorage": {
      "Archive": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },

      "Levels": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      },

      "Winrooms": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      }
    },

    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}