{
  "name": "basic-jtoh-player",
  "tree": {
    "$className": "DataModel",
    "$ignoreUnknownInstances": true,

    "ReplicatedStorage": {
      "DamageEvent": {
        "$className": "RemoteEvent"
      },

      "createInstance": {
        "$path": "src/shared/createInstance.lua"
      },

      "Roact": {
        "$path": "lib/Roact"
      },

      "Signal": {
        "$path": "lib/Signal.lua"
      },

      "BasicJToHPlayer": {
        "$path": "src/shared",

        "Remote": {
          "$className": "Folder",
  
          "OnPlayerWin": {
            "$className": "RemoteEvent"
          },
          
          "RequestLevel": {
            "$className": "RemoteFunction"
          },
  
          "StopLevel": {
            "$className": "RemoteFunction"
          }
        }
      }
    },

    "StarterGui": {
      "$path": "src/gui",
      "$ignoreUnknownInstances": true
    },

    "StarterPlayer": {
      "StarterCharacterScripts": {
        "$path": "src/character",
        "$ignoreUnknownInstances": true
      },

      "StarterPlayerScripts": {
        "COScriptRepo": {
          "$className": "Folder",
          "$ignoreUnknownInstances": true,

          "JToH": {
            "$path": "lib/JToH/clientObjects"
          },

          "BasicJToHPlayer": {
            "$path": "src/clientObjects"
          }
        },

        "BasicJToHPlayer": {
          "$path": "src/client"
        },

        "PlayerModule": {
          "$path": "lib/PlayerModule"
        }
      }
    },

    "ServerScriptService": {
      "BasicJToHPlayer": {
        "$path": "src/server"
      },

      "JToH": {
        "$path": "lib/JToH/server"
      }
    },

    "ServerStorage": {
      "Archive": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },

      "Levels": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      },

      "Winrooms": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      }
    },

    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}